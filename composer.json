        "TYPO3",
        "extension",
        "bookmarks"
    ],
    "homepage": "https://bitbucket.org/colorcube/bookmark-pages",
    "support": {
        "issues": "https://bitbucket.org/colorcube/bookmark-pages/issues"
    },
