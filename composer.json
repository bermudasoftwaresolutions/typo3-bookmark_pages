        {
            "name": "RenÃ© Fritz",
            "email": "r.fritz@colorcube.de",
            "role": "Developer",
            "homepage": "http://www.colorcube.de"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.6.0",
        "typo3/cms-core": "^6.2 || ^7.6 || ^8.0",
        "typo3-ter/typoscript-rendering": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Colorcube\\BookmarkPages\\": "Classes"
        }
    },
    "replace": {
